#!/usr/bin/env python
import sys

import ckan.models
# have to import this in order to have config for db connection
import ckan.tests

if __name__ == '__main__':
    action = sys.argv[1]
    print action
    if action == 'rebuild':
        ckan.models.DomainModel.rebuild()
    elif action == 'testdata':
        ckan.tests.create_test_data()
    else:
        print 'Unknown action %s' % action

